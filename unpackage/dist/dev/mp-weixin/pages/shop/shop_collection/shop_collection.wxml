<view class="list"><view class="{{['sub-list valid',subState]}}"><block wx:if="{{noDateFlag}}"><view class="no_data"><image src="/static/no_data.png" mode></image><text class="font-28 gray">没有找到相关收藏~</text></view></block><block wx:for="{{collectList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="row"><view data-event-opts="{{[['tap',[['collectDelete',[index]]]]]}}" class="menu" catchtap="__e"><image src="/static/del_white.png" mode></image></view><view data-event-opts="{{[['touchstart',[['touchStart',[index,'$event']]]],['touchmove',[['touchMove',[index,'$event']]]],['touchend',[['touchEnd',[index,'$event']]]]]}}" class="{{['carrier',typeClass=='valid'?theIndex==index?'open':oldIndex==index?'close':'':'']}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"><view class="collection_list"><view class="top_content"><image src="{{item.store_logo}}" mode data-event-opts="{{[['tap',[['enterShop',['$0'],[[['collectList','',index,'mid']]]]]]]}}" bindtap="__e"></image><view class="shop_info"><text data-event-opts="{{[['tap',[['enterShop',['$0'],[[['collectList','',index,'mid']]]]]]]}}" class="font-32 block" bindtap="__e">{{item.store_name}}</text><view class="num"><text class="font-24 gray">{{"销量"+item.sale_count}}</text><text class="font-24 gray">{{"收藏量"+item.fav_count+"次"}}</text></view></view><button data-event-opts="{{[['tap',[['enterShop',['$0'],[[['collectList','',index,'mid']]]]]]]}}" class="btn_collection_green" bindtap="__e">进店逛逛</button></view><view class="goods_list"><block wx:for="{{item.similarity}}" wx:for-item="ite" wx:for-index="inde" wx:key="inde"><view data-event-opts="{{[['tap',[['shopDetail',['$0','$1'],[[['collectList','',index],['similarity','',inde,'id']],[['collectList','',index],['similarity','',inde,'type']]]]]]]}}" class="list_view" bindtap="__e"><image src="{{ite.image}}" mode></image><text class="font-24">{{"￥"+ite.price}}</text></view></block></view></view></view></view></block><block wx:if="{{loadingMoreFlag}}"><uni-load-more vue-id="1" status="{{loadingMore}}" bind:__l="__l"></uni-load-more></block></view></view>