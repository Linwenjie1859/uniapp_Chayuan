<view class="bg-white"><block wx:for="{{listUnique}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view class="shop_info"><image src="{{item.image}}" mode></image><text class="font-28">描述相符</text><sunui-star class="sunui-star" vue-id="{{'1-'+index}}" defaultStar="{{item.product_score}}" maxStar="{{5}}" index="{{0}}" currentIndex="{{index}}" data-event-opts="{{[['^changeStar',[['changeStar']]]]}}" bind:changeStar="__e" bind:__l="__l"></sunui-star></view><textarea class="font-28 margin-top-xs" value="{{item.comment}}" placeholder="宝贝满意您的期待吗？说说怎么样" data-event-opts="{{[['input',[['__set_model',['$0','comment','$event',[]],[[['listUnique','',index]]]]]]]}}" bindinput="__e"></textarea><view class="add_pic"><robby-image-upload vue-id="{{'2-'+index}}" showUploadProgress="true" currentIndex="{{index}}" serverUrlDeleteImage="http://www.fyang.com/ebapi/public_api/delete_image" serverUrl="http://www.fyang.com/ebapi/public_api/upload" data-event-opts="{{[['^add',[['addImage']]]]}}" bind:add="__e" bind:__l="__l"></robby-image-upload></view><view class="score"><text class="font-32 title">店铺评分</text><view class="score_list"><text class="font-28">服务态度</text><sunui-star class="sunui-star" vue-id="{{'3-'+index}}" defaultStar="{{item.service_score}}" maxStar="{{5}}" index="{{3}}" currentIndex="{{index}}" data-event-opts="{{[['^changeStar',[['changeStar']]]]}}" bind:changeStar="__e" bind:__l="__l"></sunui-star></view></view></view></block><button data-event-opts="{{[['tap',[['evaluation',['$event']]]]]}}" class="btn_green" bindtap="__e">发布</button></view>