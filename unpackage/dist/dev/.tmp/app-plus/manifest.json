    "popGesture": "close",
    "launchwebview": {
      "id": "1",
      "kernel": "WKWebview",
      "uni-app": "auto"
    },
    "statusbar": {
      "immersed": "supportedDevice",
      "style": "light",
      "background": "#39b54a"
    },
    "usingComponents": true,
    "distribute": {
      "google": {
        "permissions": [
          "<uses-feature android:name=\"android.hardware.camera\"/>",
