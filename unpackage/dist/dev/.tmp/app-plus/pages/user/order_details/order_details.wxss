	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	position: fixed;
	bottom: 0rpx;
}
.order_info text {
	text-align: left;
	display: block;
	width: 650rpx;
	margin: 10rpx auto;
}

