}
.address_info {
	width: 700rpx;
	height: 150rpx;
	margin: 25rpx auto;
	background-color: #FFFFFF;
	border-radius: 10rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.address_info image {
	width: 40rpx;
	height: 40rpx;
	margin: 0rpx 20rpx 0rpx 25rpx;
}
.address_info .info_top {
	display: block;
	width: 540rpx;
	text-align: left;
	margin-bottom: 15rpx;
}
.address_info .info_top text {
	margin-right: 25rpx;
}
.order_list {
	width: 700rpx;
	margin: 0 auto;
	background-color: #FFFFFF;
	border-radius: 10rpx;
	padding-bottom: 25rpx;
	margin-bottom: 25rpx;
}
.order_list .list_top {
	width: 650rpx;
	height: 90rpx;
