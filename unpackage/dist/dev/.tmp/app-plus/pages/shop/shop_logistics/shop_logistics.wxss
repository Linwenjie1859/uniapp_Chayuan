page {
  background-color: #f2f2f2;
}
.content {
  text-align: center;
  height: 100%;
}
.top_content {
  width: 700rpx;
  height: 170rpx;
  background-color: #ffffff;
  margin: 25rpx auto;
  border-radius: 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.top_content image {
  width: 115rpx;
  height: 115rpx;
  margin-left: 25rpx;
}
.top_content .shop_info {
  width: 510rpx;
  height: 115rpx;
  margin: 0rpx 25rpx;
}
.top_content .block {
  display: block;
  text-align: left;
}
.address {
  width: 700rpx;
  height: 90rpx;
  margin: 0 auto;
  background-color: #ffffff;
  border-radius: 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.address image {
  width: 40rpx;
  height: 40rpx;
  margin: 0rpx 15rpx 0rpx 25rpx;
}
.logistics {
  width: 700rpx;
  margin: 25rpx auto;
  background-color: #ffffff;
  border-radius: 10rpx;
}

