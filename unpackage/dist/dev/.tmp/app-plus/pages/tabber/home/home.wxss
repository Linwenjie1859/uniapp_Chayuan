.header {
	width: 100%;
	height: 150rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	position: fixed;
	top: 0;
	z-index: 10;
}
.input_view {
	width: 450rpx;
	height: 50rpx;
	background-color: rgba(255, 255, 255, 0.7);
	position: absolute;
	left: 20%;
	z-index: 1024;
	border: 1px solid rgba(135, 153, 163, 0.3);
	bottom: 39rpx;
}
.input_view text {
	display: block;
	width: 450rpx;
	height: 50rpx;
	line-height: 50rpx;
	text-align: center;
}
.after {
	width: 100%;
	position: fixed;
	top: 0rpx;
	background: #39b54a;
	-webkit-transition: opacity 0.05s linear;
	transition: opacity 0.05s linear;
	color: #fff;
}
.after .middle {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	width: 400rpx;
	padding-top: 90rpx;
	margin: 0 auto;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
