page {
  width: 100%;
  height: 100%;
  background-color: #f2f2f2;
  position: relative;
}
.content {
  height: 100%;
  width: 100%;
}
.banner {
  width: 100%;
  height: 380rpx;
}
.banner .swiper {
  height: 380rpx;
}
.banner image {
  width: 100%;
  height: 380rpx;
}
.header {
  width: 100%;
  height: 170rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: fixed;
  top: 0;
  z-index: 10;
}
.after {
  width: 100%;
  height: 170rpx;
  position: fixed;
  top: 0rpx;
  -webkit-transition: opacity 0.05s linear;
  transition: opacity 0.05s linear;
  background-color: #51c77d;
  color: #fff;
}
.after .middle {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 400rpx;
  padding-top: 100rpx;
  margin: 0 auto;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.on {
  width: 80rpx;
  text-align: center;
  margin-bottom: -4rpx;
  color: #f66d3c;
  border-bottom: solid 4rpx #f66d3c;
}
.footer {
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  height: 100rpx;
  border-top: solid 1rpx #eee;
  background-color: #fff;
  z-index: 2;
}
.footer .footer_list {
  width: 700rpx;
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin: 0 auto;
}
.icons {
  width: 310rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.box {
  width: 80rpx;
  height: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.box image {
  width: 50rpx;
  height: 50rpx;
}
.btn {
  height: 80rpx;
  border-radius: 40rpx;
  overflow: hidden;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.joinCart,
.buy {
  height: 80rpx;
  width: 290rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
}
.margin-left-sm {
  margin-left: 20rpx;
}
.flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.shop_list {
  width: 700rpx;
  position: absolute;
  top: 340rpx;
  left: 50%;
  margin-left: -350rpx;
  height: 100%;
}
.shop_list .list_top {
  width: 700rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin-bottom: 25rpx;
}
.shop_list .list_top image {
  width: 35rpx;
  height: 35rpx;
  margin: 25rpx 15rpx 25rpx 25rpx;
}
.shop_list .list_top .title {
  width: 650rpx;
  display: block;
  margin: 0 auto;
  padding-top: 20rpx;
}
.shop_view {
  width: 650rpx;
  margin: 0 auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding-bottom: 25rpx;
}
.align-center {
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.shop_view .static {
