.content{text-align:center}.position-relative{display:inline-block;width:45rpx;text-align:center;height:70rpx;line-height:70rpx;border:1rpx solid;border-radius:2rpx;color:#39b54a;margin:40rpx 10rpx;overflow:hidden;position:relative;background-color:#d7f0db;font-size:36rpx;font-weight:700}.has-transition{width:100%;height:calc(100% * 10);position:relative;-webkit-transition:all .5s;transition:all .5s}.position-absolute{position:absolute;top:0;left:0;margin:0;border:none}