<view class="content"><view class="header"><view data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="input_view" bindtap="__e"><text class="font-28 gray">搜索商品</text></view><view class="after" style="{{'opacity:'+(afterHeaderOpacity)+';'+('z-index:'+(afterHeaderzIndex)+';')}}"></view></view><view class="banner"><view class="uni-padding-wrap"><view class="page-section swiper"><view class="page-section-spacing"><swiper class="swiper" indicator-color="#fff" indicator-active-color="#51c77d" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{farmProduceList.banner}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image src="{{item.img}}" mode></image></swiper-item></block></swiper></view></view></view></view><block wx:for="{{farmProduceList.merInfo}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="benefit_list"><text class="title font-32">{{"————"+item.name+"————"}}</text><image src="{{item.image}}" mode></image><view class="list_down"><block wx:for="{{item.list}}" wx:for-item="ite" wx:for-index="ind" wx:key="ind"><view class="down_view"><image src="{{ite.store_logo}}" mode data-event-opts="{{[['tap',[['detail',['$0'],[[['farmProduceList.merInfo','',index],['list','',ind,'id']]]]]]]}}" bindtap="__e"></image><view data-event-opts="{{[['tap',[['detail',['$0'],[[['farmProduceList.merInfo','',index],['list','',ind,'id']]]]]]]}}" class="view_details" bindtap="__e"><text class=" font-28">{{ite.store_name}}</text><text class=" font-24">详情></text></view><view class="view_details font-24"><view class="details_left"><image src="/static/navigation_icon.png" mode></image><text data-event-opts="{{[['tap',[['navToMap',['$0'],[[['farmProduceList.merInfo','',index],['list','',ind]]]]]]]}}" bindtap="__e">导航</text></view><view data-event-opts="{{[['tap',[['keep',['$event']]]]]}}" class="details_right" bindtap="__e"><image src="/static/collection_icon.png" mode></image><text class="white">{{(ite.is_collect?'':'已')+"收藏"}}</text></view></view></view></block></view></view></block></view>