
page{
	background-color: #F2F2F2;
}
.time{
	width: 700rpx;
	margin: 25rpx auto;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.time text{
	display: block;
	margin: 0 auto;
	padding: 2rpx 25rpx;
	background-color: #E5E5E5;
	border-radius: 25rpx;
}
.new_list{
	width: 700rpx;
	margin: 25rpx auto;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.new_list image{
	width: 80rpx;
	height: 80rpx;
	border-radius: 50%;
}
.new_list text{
	width: 540rpx;
	display: block;
	background-color: #FFFFFF;
	border-radius: 10rpx;
	padding: 15rpx 25rpx;
	margin-left: 25rpx;
}

