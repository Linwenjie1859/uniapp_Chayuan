
.content {
	text-align: center;
}
.text {
	display: block;
	width: 550rpx;
	margin: 0 auto;
	text-align: left;
	margin-top: 60rpx;
	margin-bottom: 100rpx;
}
.input_view {
	width: 550rpx;
	height: 80rpx;
	line-height: 80rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin: 0rpx auto;
	margin-top: 80rpx;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	border: 1rpx solid #aaaaaa ;
	padding: 0rpx 20rpx;
	border-radius: 8rpx;
	color: #8799a3;
	/* margin-bottom: 50upx; */
}
.input_view input {
	width: 400rpx;
	text-align: left;
}

